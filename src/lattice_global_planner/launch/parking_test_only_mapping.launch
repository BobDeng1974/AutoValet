<!-- 
  Simulate a carlike robot with the teb_local_planner in stage:
  - stage
  - map_server
  - move_base
  - static map
  - amcl
  - rviz view
 -->

<launch>

	<!--  ************** Global Parameters ***************  -->
    <!-- <param name="/use_sim_time" value="true"/> -->

	<!--  ************** Gazebo Simulator ***************  -->
